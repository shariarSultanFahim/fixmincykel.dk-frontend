{
  "name": "fam",
  "version": "16.0.0",
  "private": true,
  "description": "Production-ready Next.js 16+ starter built with Tailwind CSS 4 and TypeScript.",
  "author": {
    "name": "Shariar Sultan",
    "email": "shariarsfahim@gmail.com",
    "url": "https://fa-m.dev"
  },
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build --webpack && open .next/analyze/nodejs.html",
    "clean": "rimraf .next",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "knip": "knip",
    "lint": "eslint . --ext js,ts,tsx --cache",
    "lint:fix": "eslint . --ext js,ts,tsx --fix --cache && prettier --write .",
    "lint:ci": "eslint . --ext js,ts,tsx --max-warnings=0 && prettier --check .",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next/bundle-analyzer": "^16.0.1",
    "@next/third-parties": "^16.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.548.0",
    "next": "^16.0.7",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.65.0",
    "react-wrap-balancer": "^1.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "usehooks-ts": "^3.1.1",
    "webpack": "^5.102.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "commitizen": "^4.3.0",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "knip": "^5.66.4",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omergulcicek/nizam.git"
  },
  "homepage": "https://github.com/omergulcicek/nizam",
  "bugs": {
    "url": "https://github.com/omergulcicek/nizam/issues"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "nextjs16",
    "boilerplate",
    "starter",
    "template",
    "typescript",
    "react",
    "tailwindcss",
    "shadcn",
    "framework",
    "nizam"
  ]
}
